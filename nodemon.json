{
    "verbose": true,
    "ignore": [
        ".git/",
        ".sass-cache/",
        "_sandbox/",
        "app/views/",
        "bower/",
        "node_modules/",
        "public/",
        "test/",
        "vendor/",
        "data/",
        "ghost-github/",
        "ghost/themes/liatandco/partials/views/",
        "*.mmm",
        "*.hbs"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "execMap": {
        "js": "node --harmony --debug"
    },
    "ext": "js json"
}